{
  "expo": {
    "name": "expo-router",
    "slug": "expo-router",
    "version": "1.0.0",
    
      "scheme": "expo-router",
      "web": {
        "bundler": "metro",
        "output": "static",
        "favicon": "./assets/favicon.png"
      },
      "plugins": ["expo-router"],
      "experiments": {
        "typedRoutes": true
      },
    
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}